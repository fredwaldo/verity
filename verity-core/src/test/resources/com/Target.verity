class Person {
  final String name;
  final int age;

  public Person(String n, int a) {
    name = n;
    age = a;
  }
}

class SetName {}
class SetAge {}

class PersonBuilder {
  private String name;
  private int age;

  public static Person build(BuildPerson bp) {
    bp.buildPerson(this);
    return new Person(name, age);
  }

  public void setName(String n)(proof verity.lang.NotProven<SetName> ev) {
    name = n;
  }

  public void setAge(int a)(proof verity.lang.NotProven<SetAge> ev) {
    age = a;
  }
}


abstract class BuildPerson {
  abstract void buildPerson(PersonBuilder p) proof SetName, SetAge;
}

class BuildFoo extends BuildPerson {
  void buildPerson(PersonBuilder p) proof SetName, SetAge {
    p.setName("Foo");
    p.setAge(0);
  }
}

